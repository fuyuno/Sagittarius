{
  "name": "sagittarius",
  "version": "0.0.1",
  "description": "Desktop application for ChatWork.",
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
    "build": "gulp build",
    "prestart": "npm run build",
    "start": "electron .",
    "prepack": "npm run build",
    "pack": "electron-packager . Sagittarius --platform=all --arch=x64 --version=0.36.1 --out=dist --ignore={.vscode,bin,dist,src,screenshots}/* --prune=true --asar=true"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuyuno/Sagittarius.git"
  },
  "keywords": [],
  "author": "Mikazuki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fuyuno/Sagittarius/issues"
  },
  "homepage": "https://github.com/fuyuno/Sagittarius#readme",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "jquery": "^2.1.4",
    "node-notifier": "^4.4.0",
    "punycode": "^1.4.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "rx": "^4.0.7",
    "semantic-ui": "^2.1.7"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "coveralls": "^2.11.8",
    "del": "^2.2.0",
    "electron-connect": "^0.3.4",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.1",
    "gulp-typescript": "^2.10.0",
    "gulp-watch": "^4.3.5",
    "mocha-lcov-reporter": "^1.2.0",
    "run-sequence": "^1.1.5"
  }
}
